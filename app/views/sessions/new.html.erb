
<div id="new-item-form" class="container">
  <h1>Login</h1>
    <%= form_for @user, url: login_path, method: :post do |f| %>
      <label>Username:</label><%= f.text_field :username %><br>
      <label>Password:</label><%= f.password_field :password %>
      <%= f.submit "Login" %>
    <%end%>
</div>
